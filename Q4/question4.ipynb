    "def fac(num):\n",
    "    if num == 0:\n",
    "        return 1\n",
    "    if num == 1:      \n",
    "        return 1    \n",
    "    return num * fac(num - 1)        \n",
    "\n",
    "def main():\n",
    "    for i in range(0, 15):\n",
    "        if((i%2)==0):\n",
    "            print(\"Factorial of\", i, \":\",fac(i))\n",
    "   \n",
    " \n",
    "if __name__ == '__main__':\n",
    "    main()"
   ]
